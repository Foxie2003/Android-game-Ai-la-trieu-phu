<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="10dp"
        android:height="10dp"
        android:left="5dp"
        android:right="5dp">
        <rotate
            android:fromDegrees="45"
            android:pivotY="50%"
            android:pivotX="50%">
            <shape android:shape="rectangle">
                <solid android:color="#CF4800"/>
            </shape>
        </rotate>
    </item>
</layer-list>