<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="28dp"
        android:height="28dp"
        android:left="6dp"
        android:top="6dp">
        <rotate android:fromDegrees="135">
            <shape android:shape="rectangle">
                <gradient android:startColor="#0F4B67"
                    android:centerColor="#050522"
                    android:endColor="#0F4B67"
                    android:angle="45"/>
            </shape>
        </rotate>
    </item>
    <item android:width="28dp"
        android:height="28dp"
        android:left="366dp"
        android:top="6dp">
        <rotate android:fromDegrees="135">
            <shape android:shape="rectangle">
                <gradient android:startColor="#0F4B67"
                    android:centerColor="#050522"
                    android:endColor="#0F4B67"
                    android:angle="45"/>
            </shape>
        </rotate>
    </item>
    <item
        android:width="360dp"
        android:height="40dp"
        android:left="20dp"
        android:right="20dp">
        <shape android:shape="rectangle">
            <gradient android:startColor="#0F4B67"
                android:centerColor="#050522"
                android:endColor="#0F4B67"
                android:angle="90"/>
        </shape>
    </item>
</layer-list>